---
layout: false
directory_index: false
---
<% pages = sitemap.resources.find_all{ |p| !p.source_file.nil? && p.source_file.match(/\.html/) && !p.data.sitemap_noindex } %>
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
<% pages.each do |page| %>
  <url>
    <loc>https://arandilopez.me/<%=page.destination_path.gsub('/index.html','')%></loc>
    <lastmod><%= Date.today.to_time.iso8601 %></lastmod>
    <changefreq><%= page.data.changefreq || "monthly" %></changefreq>
    <priority><%= page.data.priority || "0.5" %></priority>
  </url>
<% end %>
</urlset>
